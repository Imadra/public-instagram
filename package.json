{
	"name": "public-instagram",
	"version": "1.0.0",
	"description": "Tool to fetch Instagram's public content.",
	"main": "lib/index.js",
	"module": "es/index.js",
	"files": [
		"lib",
		"es"
	],
	"scripts": {
		"build:commonjs": "BABEL_ENV=commonjs babel src -d lib --ignore spec.js,test.js",
		"build:es": "BABEL_ENV=es babel src -d es --ignore spec.js,test.js",
		"build": "npm run build:commonjs && npm run build:es",
		"clean": "rm -rf es lib node_modules",
		"commitmsg": "commitlint -e $GIT_PARAMS",
		"lint": "eslint src",
		"precommit": "lint-staged"
	},
	"keywords": [
		"instagram",
		"scraper",
		"public",
		"api"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ivolimasilva/public-instagram.git"
	},
	"author": {
		"name": "Ivo Lima Silva",
		"email": "ivolimasilva@outlook.com",
		"url": "http://ivolimasilva.xyz"
	},
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/ivolimasilva/public-instagram/issues"
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"homepage": "https://github.com/ivolimasilva/public-instagram#readme",
	"dependencies": {
		"isomorphic-fetch": "^2.2.1",
		"yuri": "^1.0.3"
	},
	"devDependencies": {
		"@commitlint/cli": "^6.2.0",
		"@commitlint/config-conventional": "^6.1.3",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.3",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-moxy": "^2.3.3",
		"eslint": "^4.19.1",
		"eslint-config-moxy": "^5.4.1",
		"husky": "^0.14.3",
		"lint-staged": "^7.1.2"
	}
}
